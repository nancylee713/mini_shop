<h1>All Merchants</h1>

<%= link_to "New Merchant", "/merchants/new" %>

<% @merchants.each do |merchant| %>
  <h2><%= link_to merchant.name, "/merchants/#{merchant.id}" %></h2>
  <p><%= merchant.address %></p>
  <p><%= merchant.city %></p>
  <p><%= merchant.state %></p>
  <p><%= merchant.zip %></p>
<% end %>
