<%= form_tag("/merchants/#{@merchant.id}", method: :patch) do %>
  <p><%= label_tag :name %></p>
  <p><%= text_field_tag :name %></p>
  <p><%= label_tag :address %></p>
  <p><%= text_field_tag :address %></p>
  <p><%= label_tag :city %></p>
  <p><%= text_field_tag :city %></p>
  <p><%= label_tag :state %></p>
  <p><%= text_field_tag :state %></p>
  <p><%= label_tag :zip %></p>
  <p><%= text_field_tag :zip %></p>
  <p><%= submit_tag "Update Merchant" %></p>
<% end %>
